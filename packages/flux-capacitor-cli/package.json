{
  "name": "flux-capacitor-cli",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "build": "babel src/ --out-dir lib/ --ignore __tests__ --source-maps",
    "test": "standard src/ && ava",
    "watch": "babel -w src/ --out-dir lib/ --ignore __tests__ --source-maps",
    "prepublish": "npm run build && npm test",
    "pretest": "npm run build"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "ignore": [
      "lib/"
    ],
    "presets": [
      "es2015-node4",
      "es2016",
      "es2017"
    ]
  },
  "engines": {
    "node": ">= 4.0"
  },
  "author": "Andy Wermke <andy@dev.next-step-software.com>",
  "repository": "flux-capacitor/flux-capacitor",
  "bugs": "https://github.com/flux-capacitor/flux-capacitor/issues",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-watch": "^2.0.3",
    "standard": "^8.4.0"
  },
  "dependencies": {
    "fs-extra": "^1.0.0",
    "json-update": "^3.0.0",
    "meow": "^3.7.0",
    "mkdirp-promise": "^4.0.1",
    "mz": "^2.5.0",
    "path-exists": "^3.0.0",
    "uniq": "^1.0.1"
  }
}
